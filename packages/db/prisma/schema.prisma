// This is your Prisma schema file,
// learn more about it in the docs  https //pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgres"
    url      = env("DATABASE_URL")
}

model Post {
    id      String @id @default(cuid())
    title   String
    content String
}
model Session {
    id          String @id @default(cuid())
    createdAt DateTime @default(now())
    name String        @default("NO_NAME")
    userId String
    gyroT Float[]
    gyroX Float[]
    gyroY Float[]
    gyroZ Float[]
    accelerationT Float[]
    accelerationX Float[]
    accelerationY Float[]
    accelerationZ Float[]
}
